<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

<Script file="gcd.lua"/>

<Frame name="AssiduityGCD" parent="UIParent" hidden="false" enableMouse="false">
    <Size x="150" y="150"/>
    <Anchors>
        <Anchor point="CENTER"/>
    </Anchors>
    <Frames>
        <Cooldown parentKey="CD" inherits="CooldownFrameTemplate" hidden="false" setAllPoints="true">
            <Size x="150" y="150"/>
            <Anchors>
                <Anchor point="CENTER"/>
            </Anchors>
        </Cooldown>
    </Frames>
    <Scripts>
        <OnLoad function="AssiduityGCD_OnLoad"/>
    </Scripts>
</Frame>

</Ui>