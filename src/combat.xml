<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

<Script file="combat.lua"/>

<Frame name="AssiduityCombat" enableMouse="false" hidden="true" parent="UIParent">
    <Size x="30" y="30"/>
    <Anchors>
        <Anchor point="BOTTOMRIGHT" relativeTo="UIParent" relativePoint="BOTTOM">
            <Offset x="-380" y="350"/>
        </Anchor>
    </Anchors>
    <Layers>
        <Layer>
            <Texture file="Interface\Icons\Ability_DualWield" setAllPoints="true"/>
            <FontString parentKey="timerFS" inherits="AssiduityLargeYellowFont">
                <Anchors>
                    <Anchor point="CENTER"/>
                </Anchors>
            </FontString>
        </Layer>
    </Layers>
    <Scripts>
        <OnLoad function="AssiduityCombat_OnLoad"/>
    </Scripts>
</Frame>

</Ui>